  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug WAB",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/wab"
    },    
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickGoProcess}"
    }
  ]
}